[DEFAULT]
aws_access_key={{ hostvars[iam_user].user_aws_access_key }}
aws_secret_key={{ hostvars[iam_user].user_aws_secret_key }}
{% for var, queue in queues.iteritems() %}
{{ var }}={{ hostvars[queue].region }}:{{ hostvars[queue].name }}
{% endfor %}
{% for var, bucket in buckets.iteritems() %}
{{ var }}={{ hostvars[bucket].region }}:{{ hostvars[bucket].name }}:{{ hostvars[bucket].s3_storage_class }}
{% endfor %}
{% for var, topic in topics.iteritems() %}
{{ var }}={{ hostvars[topic].region }}:{{ hostvars[topic].arn }}
{% endfor %}
